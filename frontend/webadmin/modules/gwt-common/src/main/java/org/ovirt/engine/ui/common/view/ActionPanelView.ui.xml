<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:w="urn:import:org.ovirt.engine.ui.common.widget">

    <g:FlowPanel ui:field="mainContainer" addStyleNames="toolbar-pf hidden-kebab">
        <g:FlowPanel>
            <g:FlowPanel ui:field="actionContainer" addStyleNames="toolbar-pf-actions">
                <b:FormGroup ui:field="searchBarContainer" addStyleNames="toolbar-pf-filter" visible="false" />
                <b:FormGroup ui:field="actionFormGroup">
                    <w:Kebab ui:field="actionKebab" visible="false" />
                </b:FormGroup>
            </g:FlowPanel>
            <b:Container ui:field="filterResults" fluid="true"/>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>