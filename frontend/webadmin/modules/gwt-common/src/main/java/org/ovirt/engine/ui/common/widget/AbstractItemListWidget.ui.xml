<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:gb="urn:import:org.gwtbootstrap3.client.ui.html">

    <g:FlowPanel>
        <b:Row>
            <b:Column size="SM_6">
                <g:Label ui:field="itemListLabel" addStyleNames="form-group" />
            </b:Column>
            <b:Column size="SM_6">
                <g:FlowPanel ui:field="itemListPanel">
                    <gb:UnorderedList ui:field="itemList" />
                </g:FlowPanel>
            </b:Column>
        </b:Row>
    </g:FlowPanel>
</ui:UiBinder>