<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <g:FlowPanel ui:field="widgetWrapper" addStyleNames="toast-pf notif_toast alert alert-dismissable">
        <b:Button ui:field="closeButton" addStyleNames="close notif_closeIcon" />
        <b:html.Span ui:field="icon" addStyleNames="pficon pficon-ok" />
        <b:html.Span ui:field="label" />
    </g:FlowPanel>

</ui:UiBinder>
