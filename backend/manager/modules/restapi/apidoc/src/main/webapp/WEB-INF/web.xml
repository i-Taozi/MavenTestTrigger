<web-app
    xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">

    <welcome-file-list>
        <welcome-file>model.html</welcome-file>
    </welcome-file-list>

    <filter>
        <filter-name>doc</filter-name>
        <filter-class>org.ovirt.engine.api.restapi.doc.DocFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>doc</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>

</web-app>
