{
  "name": "coopr-ui",
  "version": "1.4.0",
  "description": "Front-end for Coopr",
  "repository": {
    "type": "git",
    "url": "http://github.com/cdapio/coopr.git"
  },
  "scripts": {
    "start": "node ./server.js",
    "backend": "COOPR_USE_DUMMY_PROVISIONER=true COOPR_DISABLE_UI=true ../coopr-standalone/target/coopr-0.9.9-SNAPSHOT-standalone/bin/coopr.sh",
    "prebuild": "npm install && ./node_modules/bower/bin/bower update --config.interactive=false --allow-root",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "test": "node ./node_modules/karma/bin/karma start test/karma-conf.js",
    "test-single-run": "node ./node_modules/karma/bin/karma start test/karma-conf.js --no-auto-watch --single-run"
  },
  "devDependencies": {
    "bower": "^1.3.10",
    "del": "^1.0.0",
    "gulp": "^3.8.9",
    "gulp-angular-templatecache": "^1.3.0",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-concat": "^2.3.4",
    "gulp-debug": "^1.0.1",
    "gulp-if": "^1.2.4",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^1.3.3",
    "gulp-livereload": "^2.1.0",
    "gulp-load-plugins": "^0.7.0",
    "gulp-minify-css": "^0.3.8",
    "gulp-ng-annotate": "^0.4.0",
    "gulp-nodemon": "^1.0.4",
    "gulp-replace": "^0.5.0",
    "gulp-rev": "^2.0.1",
    "gulp-size": "^1.0.0",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.0",
    "gulp-wrapper": "^0.1.5",
    "jasmine-core": "2.x",
    "karma": "^0.12.21",
    "karma-chrome-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.0",
    "main-bower-files": "^2.1.0",
    "merge-stream": "^0.1.5"
  },
  "dependencies": {
    "express": "^4.9.7",
    "finalhandler": "^0.3.0",
    "http-proxy": "^1.7.1",
    "morgan": "^1.3.1",
    "serve-favicon": "^2.1.5"
  }
}
